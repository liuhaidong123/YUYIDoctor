<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_setting_"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@color/L_color_fg"
    android:layout_height="match_parent"
    tools:context="com.doctor.yuyi.activity.My_setting_Activity">
    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/activity_titleinclude"></include>
    <LinearLayout
        android:paddingLeft="15dp"
        android:background="@color/color_white"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
<RelativeLayout
    android:clickable="true"
    android:id="@+id/my_setting_rela_yijian"
    android:layout_width="match_parent"
    android:layout_height="60dp">
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_centerVertical="true"
    android:text="意见反馈"
    android:textColor="@color/L_color_textNomol"/>
    <ImageView
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/many"/>
</RelativeLayout>



        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/L_color_fg"/>

        <RelativeLayout
            android:clickable="true"
            android:id="@+id/my_setting_rela_lianxi"
            android:layout_width="match_parent"
            android:layout_height="60dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="联系我们"
                android:textColor="@color/L_color_textNomol"/>
            <ImageView
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/many"/>
        </RelativeLayout>



        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/L_color_fg"/>

        <RelativeLayout
            android:clickable="true"
            android:id="@+id/my_setting_rela_about"
            android:layout_width="match_parent"
            android:layout_height="60dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="关于宇医"
                android:textColor="@color/L_color_textNomol"/>
            <ImageView
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/many"/>
        </RelativeLayout>
    </LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/my_setting_textV_exit"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="44dp"
            android:text="退出登录"
            android:textColor="@color/L_color_exit_back"
            android:gravity="center"
            android:textSize="17sp"
            android:layout_centerHorizontal="true"
            android:background="@drawable/my_setting_exit"
            />
    </RelativeLayout>
</LinearLayout>
