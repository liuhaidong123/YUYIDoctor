<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:paddingLeft="20dp"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp">
        <com.doctor.yuyi.lzh_utils.RoundImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@mipmap/doc"
            android:layout_centerVertical="true"
            android:id="@+id/my_patientList_image"
            />
        <TextView
            android:layout_marginLeft="15dp"
            android:text="张三"
            android:textColor="#333333"
            android:textSize="14sp"
            android:layout_toRightOf="@+id/my_patientList_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:id="@+id/my_patientList_textVname"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingRight="10dp"
            android:paddingLeft="10dp"
            android:src="@mipmap/many"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>

</RelativeLayout>