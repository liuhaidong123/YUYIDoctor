<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_forum_posts_"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.doctor.yuyi.activity.My_forumPosts_Activity">
    <RelativeLayout
        android:id="@+id/ms_tops_relayout"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
        <ImageView
            android:clickable="true"
            android:onClick="reBack"
            android:id="@+id/activity_include_imageReturns"
            android:layout_alignParentLeft="true"
            android:paddingStart="15dp"
            android:paddingEnd="15dp"
            android:layout_width="wrap_content"
            android:layout_centerVertical="true"
            android:layout_height="wrap_content"
            android:src="@mipmap/ms_home_return"/>
        <TextView
            android:id="@+id/titleinclude_textviews"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的帖子"
            android:layout_centerInParent="true"
            android:textColor="@color/L_color_titleText"
            android:textSize="17sp"/>
        <TextView
            android:id="@+id/my_forum_posts_post"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#25f368"
            android:text="发帖"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:paddingRight="15dp"
            android:paddingLeft="15dp"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"/>
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/color_bg"/>
    <ListView
        android:divider="#eeeeee"
        android:dividerHeight="1dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/my_forum_posts_listview"></ListView>
</LinearLayout>
